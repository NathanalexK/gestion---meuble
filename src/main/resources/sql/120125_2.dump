           FROM public.calculer_somme_poste_fille() calculer_somme_poste_fille(id_compte_mere, id_mere, somme_montant)
          GROUP BY calculer_somme_poste_fille.id_mere) cpl ON ((poste.id_poste = cpl.id_mere)));


