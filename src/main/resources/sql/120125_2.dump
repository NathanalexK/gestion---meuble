--
-- Name: liste_general _RETURN; Type: RULE; Schema: public; Owner: postgres
--

CREATE OR REPLACE VIEW public.liste_general AS
 SELECT row_number() OVER () AS id,
    r.id_exercice AS id_exeercice,
    tr.libelle,
    tr.id_type_rubrique,
    sum((r.prix_unitaire * r.quantite)) AS total_rubrique,
    tr.incorporabilite
   FROM (public.rubrique r
     JOIN public.type_rubrique tr ON ((r.id_type_rubrique = tr.id_type_rubrique)))
  GROUP BY r.id_exercice, tr.libelle, tr.id_type_rubrique;


