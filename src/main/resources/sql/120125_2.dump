--
-- Name: list_analytique; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.list_analytique AS
 SELECT row_number() OVER () AS id,
    lg.id_type_rubrique,
    imputation.pourcentage,
    imputation.id_centre,
    ((lg.total_rubrique / (100)::numeric) * imputation.pourcentage) AS total_par_centre,
    lg.incorporabilite
   FROM (public.imputation
     JOIN public.liste_general lg ON ((imputation.id_type_rubrique = lg.id_type_rubrique)))
  GROUP BY imputation.id_centre, imputation.pourcentage, lg.id_type_rubrique, lg.total_rubrique, lg.incorporabilite;


