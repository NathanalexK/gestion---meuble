--
-- Name: v_poste_fille; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_poste_fille AS
 SELECT pf.id_poste_fille,
    pf.id_mere,
    pf.id_compte_mere,
    pf.compte,
    pf.libelle,
    COALESCE(pfv.montant, (0)::numeric) AS montant
   FROM (public.poste_fille pf
     LEFT JOIN public.poste_fille_value pfv ON ((pf.id_poste_fille = pfv.compte)))
  ORDER BY pf.id_poste_fille;


ALTER TABLE public.v_poste_fille OWNER TO postgres;

--
-- Name: v_poste_fille_montant; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_poste_fille_montant AS
 SELECT pf.id_poste_fille,
    pf.id_mere,
    pf.id_compte_mere,
    pf.compte,
    pf.libelle,
    (COALESCE(pfv.montant, (0)::numeric) + public.calculer_montants_recurrents_par_fille(pf.compte)) AS montant,
    1 AS id_exercice
   FROM (public.poste_fille pf
     LEFT JOIN public.poste_fille_value pfv ON ((pf.id_poste_fille = pfv.compte)))
  ORDER BY pf.id_poste_fille;


ALTER TABLE public.v_poste_fille_montant OWNER TO postgres;

--
-- Name: v_repartition; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_repartition AS
 SELECT row_number() OVER () AS row_number,
    by_centre.id_centre,
    by_centre.id_exercice,
    by_centre.nom,
    by_centre.montant,
    by_centre.categorie,
    round(((by_centre.montant * (100)::numeric) / operationnel.operationnel), 2) AS cle,
    ((structure.structure * round(((by_centre.montant * (100)::numeric) / operationnel.operationnel), 2)) / (100)::numeric) AS structure,
    (by_centre.montant + ((structure.structure * round(((by_centre.montant * (100)::numeric) / operationnel.operationnel), 2)) / (100)::numeric)) AS cout_total
   FROM ((( SELECT analyse_ensemble.id_centre,
            analyse_ensemble.id_exercice,
            analyse_ensemble.nom,
            sum(analyse_ensemble.reel) AS montant,
            analyse_ensemble.categorie
           FROM public.analyse_ensemble
          GROUP BY analyse_ensemble.id_centre, analyse_ensemble.id_exercice, analyse_ensemble.categorie, analyse_ensemble.nom
         HAVING (analyse_ensemble.categorie = 1)) by_centre
     JOIN ( SELECT sum(analyse_ensemble.reel) AS operationnel
           FROM public.analyse_ensemble
          WHERE (analyse_ensemble.categorie = 1)) operationnel ON (true))
     JOIN ( SELECT sum(analyse_ensemble.reel) AS structure
           FROM public.analyse_ensemble
          WHERE (analyse_ensemble.categorie = 0)) structure ON (true));


ALTER TABLE public.v_repartition OWNER TO postgres;

--
-- Name: v_repartition_total; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_repartition_total AS
 SELECT row_number() OVER () AS id,
    v_repartition.id_exercice,
    sum(v_repartition.montant) AS s_direct,
    sum(v_repartition.structure) AS s_structure,
    sum(v_repartition.cout_total) AS s_cout_total
   FROM public.v_repartition
  GROUP BY v_repartition.id_exercice;


ALTER TABLE public.v_repartition_total OWNER TO postgres;

--
-- Name: v_variable_fixe; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_variable_fixe AS
 SELECT row_number() OVER () AS row_number,
    rubrique.id_exercice,
    type_rubrique.nature,
    sum((rubrique.prix_unitaire * rubrique.quantite)) AS valeur
   FROM (public.rubrique
     JOIN public.type_rubrique ON ((rubrique.id_type_rubrique = type_rubrique.id_type_rubrique)))
  GROUP BY rubrique.id_exercice, type_rubrique.nature;


ALTER TABLE public.v_variable_fixe OWNER TO postgres;

